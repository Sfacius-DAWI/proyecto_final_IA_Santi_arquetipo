  "description": "Arquetipo de desarrollo para aplicaci√≥n web",
  "devDependencies": {
    "@types/node": "^22.15.14",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@firebase/analytics": "^0.10.13",
    "@firebase/app": "^0.12.1",
    "@firebase/auth": "^1.10.2",
    "@sinclair/typebox": "^0.34.33",
    "firebase": "^11.7.1",
